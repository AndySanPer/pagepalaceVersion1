     libros.`AñoPublicacion` AS libros_AñoPublicacion,
     libros.`NoEjemplareson` AS libros_NoEjemplareson,
     libros.`Idioma` AS libros_Idioma,
     autores.`ID_Autor` AS autores_ID_Autor,
