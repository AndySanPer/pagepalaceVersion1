	<queryString language="SQL">
		<![CDATA[SELECT
     libros.`ID_Libro` AS libros_ID_Libro,
     libros.`Titulo` AS libros_Titulo,
     libros.`ID_Autor` AS libros_ID_Autor,
     libros.`Editorial` AS libros_Editorial,
